    def spider_run(self):
        self.pushButton.setText("正在爬取。。。")
        self.pushButton.setEnabled(False)
        time.sleep(1)
        start_time = time.time()
        print("开始爬取！")
        spide.get_recuite_info(str(self.textEdit.toPlainText()),self.comboBox_3.currentText(),int(self.textEdit_2.toPlainText()))
        end_time = time.time()
        QMessageBox.information(self,
                                "提醒",
                                ("本次爬取_城市：{} 职位：{} 页数：{} 完毕。爬取完毕的数据存储在程序根目录下，本次爬取所用时间：{}秒").format(self.textEdit.toPlainText(),self.comboBox_3.currentText(),self.textEdit_2.toPlainText(),float(end_time-start_time)),
                                QMessageBox.Yes | QMessageBox.No)
        self.pushButton.setText("开始爬取")
        self.pushButton.setEnabled(True)

